{
    "name": "@baidu-log/element-ui",
    "version": "0.1.6",
    "description": "A Component Library for Vue.js.",
    "module": "dist/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "rollup -c -w",
        "build": "rollup -c",
        "theme:dev": "sass --quiet element-variables.scss dist/index.css -w --no-source-map",
        "theme:build": "sass --quiet element-variables.scss dist/index.css --no-source-map --style compressed",
        "prepublishOnly": "npm run theme:build && npm run build"
    },
    "dependencies": {
        "async-validator": "~1.8.1",
        "dayjs": "^1.11.13",
        "lodash": "^4.17.21",
        "normalize-wheel": "^1.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.26.10",
        "@rollup/plugin-alias": "^5.1.1",
        "@rollup/plugin-babel": "^6.0.4",
        "@rollup/plugin-commonjs": "^28.0.2",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^16.0.0",
        "@rollup/plugin-replace": "^6.0.2",
        "@rollup/plugin-terser": "^0.4.4",
        "@vue/babel-preset-jsx": "^1.4.0",
        "element-theme-chalk": "^2.15.14",
        "rollup": "^4.34.8",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-cleanup": "^3.2.1",
        "rollup-plugin-vue": "5.1.9",
        "sass": "^1.85.1",
        "vue": "2.6.14",
        "vue-template-compiler": "2.6.14"
    }
}
